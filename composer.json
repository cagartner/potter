{
    "name": "potterywp/potter",
    "description": "Set of tools that help you make WordPress sites more easily.",
    "license": "MIT",
    "authors": [{
        "name": "Vinicius Reis",
        "email": "contato@luizvinicius.com.br"
    }],
    "repositories": [{
        "type": "vcs",
