        "potterywp/super-cpt": "dev-master"
    },
    "minimum-stability": "dev",
    "autoload": {
        "files": ["src/Potter/Loader.php"],
        "psr-0": {
            "Potter": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
